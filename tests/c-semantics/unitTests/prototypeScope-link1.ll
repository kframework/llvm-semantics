  %1 = bitcast %struct.s* %c to i64*
  store i64 %c.coerce, i64* %1, align 1
  %2 = getelementptr inbounds %struct.s* %c, i32 0, i32 0
  %3 = load i32* %2, align 4
  ret i32 %3
}

